create table BranchDetails(
                [branchID] [int] NOT NULL,
    [BranchName] [varchar](30) ,
    [Location] [varchar](50) Not Null,
    [Nocustomer] [Real],
	 CONSTRAINT [PK_branchID] PRIMARY KEY CLUSTERED
(
    [branchID] ASC
)
) ON [PRIMARY]

select * from BranchDetails


